let n = 10
match $n {
    10 => println("ten"),
    20 => println("twenty"),
    _ => println("unknown")
}

let status = "success"
match $status {
    "success" => println("Operation completed"),
    "error" => println("Operation failed"),
    "pending" => println("Operation in progress"),
    _ => println("Unknown status")
}
